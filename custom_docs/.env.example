# Configuration des clés API pour Marker
# IMPORTANT: Ne jamais commiter ce fichier avec de vraies clés !
# Copiez ce fichier vers .env et ajoutez vos vraies clés

# Google Gemini API (recommandé pour qualité optimale)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Claude API (Anthropic)
CLAUDE_API_KEY=your_claude_api_key_here

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Azure OpenAI (optionnel)
AZURE_API_KEY=your_azure_api_key_here
AZURE_ENDPOINT=your_azure_endpoint_here

# AWS Bedrock (nouveau)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_SESSION_TOKEN=  # Optionnel pour credentials temporaires
AWS_REGION=us-east-1
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# Configuration Marker
TORCH_DEVICE=cuda  # auto, cuda, ou cpu
OUTPUT_DIR=./outputs
TOKENIZERS_PARALLELISM=false